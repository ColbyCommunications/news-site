{
    "name": "colbycommunications/news-site",
    "description": "Configuration project for the news.colby.edu site",
    "type": "project",
    "require": {
        "colbycommunications/colby-news-theme": "1.0.2",
        "wpackagist-plugin/header-footer": "^3.1.3",
        "wpackagist-plugin/duplicate-post": "^4.3",
        "wpackagist-plugin/wordpress-seo": "^17.8",
        "wpackagist-plugin/cloudflare": "^4.5.1",
        "johnpbloch/wordpress-core": "^5.8",
        "johnpbloch/wordpress-core-installer": "^2.0",
        "newcity/newcity-wp-wysiwyg": "dev-elementor-compatible",
        "wpackagist-plugin/accordion-blocks": "^1.3",
        "wpackagist-plugin/acf-autosize": "^2.0.11",
        "wpackagist-plugin/acf-extended": "0.8.*",
        "wpackagist-plugin/admin-bar-in-fullscreen-mode": "^1.0",
        "wpackagist-plugin/block-manager": "^1.2",
        "wpackagist-plugin/disable-comments": "^2.1",
        "wpackagist-plugin/elementor": "^3.1.4",
        "wpackagist-plugin/gallery-block-lightbox": "^1.8",
        "wpackagist-plugin/public-post-preview": "^2.9.1",
        "wpackagist-plugin/regenerate-thumbnails": "^3.1",
        "wpackagist-plugin/svg-support": "^2.3",
        "wpackagist-plugin/wp-cfm": "^1.7",
        "wpackagist-plugin/wp-graphql": "^1.6.2",
        "wpackagist-plugin/wp-nested-pages": "^3.1",
        "wpackagist-theme/twentytwentyone": "^1.4",
        "wpackagist-plugin/term-management-tools": "^2.0.1",
        "wpackagist-plugin/convert-to-blocks": "^1.0.2",
        "wpackagist-plugin/reusable-blocks-extended": "^0.8.0",
        "wpackagist-plugin/wp-saml-auth": "^1.1.0",
        "wpackagist-plugin/siteimprove": "1.2.1",
        "wpackagist-plugin/query-monitor": "^3.7.1",
        "wp-cli/wp-cli-bundle": "^2.6",
        "psy/psysh": "^0.11.2",
        "colbycommunications/advanced-custom-fields-pro": "^5.11.4",
        "colbycommunications/elementor-pro": "^3.5.2",
        "colbycommunications/jet-blocks": "^1.3.0",
        "colbycommunications/jet-blog": "^2.2.15",
        "colbycommunications/jet-elements": "^2.6.2",
        "colbycommunications/jet-menu": "^2.1.6",
        "colbycommunications/jet-popup": "^1.5.6",
        "colbycommunications/jet-search": "^2.1.17",
        "colbycommunications/jet-tabs": "^2.1.15",
        "colbycommunications/jet-tricks": "^1.4.1",
        "colbycommunications/jet-style-manager": "^1.3.2",
        "colbycommunications/jet-theme-core": "^2.0.0",
        "colbycommunications/jet-reviews": "^2.2.6",
        "colbycommunications/jet-elements-dynamic-data": "^1.3.1",
        "colbycommunications/colby-ticket": "^1.0.0"
    },
    "authors": [
        {
            "name": "Morty Mule",
            "email": "webmaster@colby.edu"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/ColbyCommunications/colby-news-theme"
        },
        {
            "type": "composer",
            "url": "https://wpackagist.org/"
        }
    ],
    "extra": {
        "wordpress-install-dir": "web/wp",
        "installer-paths": {
            "web/wp-content/mu-plugins/{$name}": [
                "type:wordpress-muplugin",
                "wpackagist-plugin/wordpress-seo"
            ],
            "web/wp-content/plugins/{$name}": [
                "type:wordpress-plugin"
            ],
            "web/wp-content/themes/{$name}": [
                "type:wordpress-theme"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "rm -rf wp-content/plugins/ahebrank/.git"
        ]
    },
    "config": {
        "allow-plugins": true
    }
}
